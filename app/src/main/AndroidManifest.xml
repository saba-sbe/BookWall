<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookwall">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bookwalllogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/bookwalllogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BasketActivity"></activity>
        <activity android:name=".CustomBookActivity" />
        <activity
            android:name=".FilterActivity"
            android:exported="true"
            android:parentActivityName=".BookActivity" />
        <activity
            android:name=".BookActivity"
            android:exported="true"
            android:parentActivityName=".MainPageActivity" />
        <activity
            android:name=".SignUpActivity"
            android:exported="true"
            android:parentActivityName=".SignInActivity" />
        <activity
            android:name=".SignInActivity"
            android:exported="true"
            android:parentActivityName=".MainPageActivity" />
        <activity android:name=".MainPageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>